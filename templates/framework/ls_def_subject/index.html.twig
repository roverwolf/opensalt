{% extends base_template %}

{% block body %}
    <h1>LsDefSubject list</h1>

    <table class="table table-condensed table-hover table-bordered table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Description</th>
                <th>Hierarchy Code</th>
                <th>Updated At</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for lsDefSubject in lsDefSubjects %}
            <tr>
                <td><a href="{{ path('lsdef_subject_show', { 'id': lsDefSubject.id }) }}">{{ lsDefSubject.id }}</a></td>
                <td>{{ lsDefSubject.title }}</td>
                <td>{{ lsDefSubject.description }}</td>
                <td>{{ lsDefSubject.hierarchyCode }}</td>
                <td>{% if lsDefSubject.updatedAt %}{{ lsDefSubject.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                    <ul class="list-inline">
                        <li>
                            <a class="btn btn-primary" href="{{ path('lsdef_subject_show', { 'id': lsDefSubject.id }) }}">show</a>
                        </li>
                        <li>
                            <a class="btn btn-default" href="{{ path('lsdef_subject_edit', { 'id': lsDefSubject.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul class="list-inline">
        <li>
            <a class="btn btn-default" href="{{ path('lsdef_subject_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
